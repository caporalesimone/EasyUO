function GetSupportedCli : String;
var
  Cnt  : Integer;
begin
  Result:=' ';
  for Cnt:=0 to High(ClientList) do
    Result:=Result+ClientList[Cnt].Cli+' ';
  Result:=LowerCase(Result);
end;

////////////////////////////////////////////////////////////////////////////////
function GetCst(Expr : TConstantNames; List : PSysVarListArray) : Cardinal;
var
  Cnt  : Integer;
begin
  Result:=0;
  if List=nil then Exit;

  Cnt:=-1;
  repeat
    Inc(Cnt);
    if List[Cnt].Expr=LISTEND then Exit;
    if List[Cnt].Expr<>Expr then Continue;
    Result:=List[Cnt].Val;
    Break;
  until False;
end;

////////////////////////////////////////////////////////////////////////////////
constructor TCstDB.Create;
begin
  inherited Create;
  CS:=TMultiReadExclusiveWriteSynchronizer.Create;
  Update('');
end;

////////////////////////////////////////////////////////////////////////////////
procedure TCstDB.Free;
begin
  CS.Free;
  inherited Free;
end;

