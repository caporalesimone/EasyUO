  List:=nil;
  if CliVer<>'' then
  begin
    CliVer:=LowerCase(CliVer);
    Cnt:=-1;
    repeat
      Inc(Cnt);
      if Cnt>High(ClientList) then Exit;
    until LowerCase(ClientList[Cnt].Cli)=CliVer;
